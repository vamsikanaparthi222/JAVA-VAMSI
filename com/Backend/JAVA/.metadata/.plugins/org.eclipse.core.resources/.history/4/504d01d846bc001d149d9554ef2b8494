package com.Backend;

public class Operators {
	public static void main(String[] args) {
		
		int i = 20;
		int j = 10;
		//Arthmatic Operators
		//System.out.println(a+b);
		//System.out.println(a-b);
		//System.out.println(a*b);
		//System.out.println(a/b);
		//System.out.println(a%b);
		
		//unary Operators
		System.out.println(i);  //20
		System.out.println(++i);  //21  //Pre Increment
		System.out.println(i++); //21  //Post Increment
		System.out.println(i);
		
		System.out.println(i);
		System.out.println(--i);
		System.out.println(i--);
		System.out.println(i);
	}

}
