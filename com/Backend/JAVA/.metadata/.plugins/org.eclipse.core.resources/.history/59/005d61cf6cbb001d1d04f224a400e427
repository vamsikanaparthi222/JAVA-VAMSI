package com.Human;

public class Human {
	public String Gender = "Boy";
	public String Color ="White";
	public String Name = "Human";
	public int Height = 170;
	public int Weight = 90;
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Human human = new Human();
		System.out.println("King is a"+human.Gender);
		System.out.println("King is a"+human.Color+"Color");
		System.out.println("He is a "+human.Name);
		System.out.println("King Height"+human.Height);
		System.out.println("King Weight"+human.Weight);

	}

}
